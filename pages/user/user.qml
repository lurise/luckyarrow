<view class="user"><view class="user-wrap"><view class="setting iconfont icon31shezhi"></view><view class="info"><block qq:if="{{getInfo}}"><view><image class="avatar" mode="aspectFill" src="{{infoRes.userInfo.avatarUrl}}"></image><view class="nickname">{{infoRes.userInfo.nickName}}</view></view></block><block qq:else><view><button class="login-btn" type="primary" size="default" open-type="getUserInfo" lang="zh_CN" data-event-opts="{{[['getuserinfo',[['getUserInfo',['$event']]]]]}}" bindgetuserinfo="__e">一键登录</button></view></block></view></view><view class="order-status"><view class="status-wrap"><view class="cell"><view class="cell-left"><image class="cell-icon" src="/static/images/user/icon-order.png" mode="aspectFill"></image><view class="cell-text title">抽奖详情</view></view><view class="iconfont iconmore1"></view></view></view></view><view class="com-item"><view class="com-wrap"><block qq:for="{{userList}}" qq:for-item="item" qq:for-index="index" qq:key="index"><view class="cell"><view class="cell-left"><image class="cell-icon" src="{{item.icon}}" mode="aspectFill"></image><view data-event-opts="{{[['tap',[['navigateTo',['$0'],[[['userList','',index,'path']]]]]]]}}" class="cell-text" bindtap="__e">{{item.title}}</view></view><view class="iconfont iconmore1"></view></view></block></view></view></view>